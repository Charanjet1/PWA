{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/public/manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "/public/sw.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/report-generator.html",
      "dest": "/public/report-generator.html"
    },
    {
      "src": "/",
      "dest": "/public/app.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/app.html"
    }
  ]
}